# jdbc.X
jdbc.driverClassName=com.mysql.jdbc.Driver
#jdbc.url=jdbc:mysql://192.168.0.13:3306/activiti?createDatabaseIfNotExist=true
jdbc.url=jdbc:mysql://localhost:3307/activiti?createDatabaseIfNotExist=true
jdbc.user=root
#jdbc.pass=83621363
jdbc.pass=root

#c3p0
#c3p0.url=jdbc:mysql://192.168.0.13:3306/activiti?useUnicode=true&characterEncoding=utf8&mysqlEncoding=utf8
c3p0.url=jdbc:mysql://localhost:3307/activiti?useUnicode=true&characterEncoding=utf8&mysqlEncoding=utf8
c3p0.user=root
#c3p0.password=83621363
c3p0.password=root
  
c3p0.driverClass=com.mysql.jdbc.Driver
c3p0.maxIdleTime=20
c3p0.maxPoolSize=200
c3p0.minPoolSize=2
c3p0.initialPoolSize=2

# hibernate.X
hibernate.connection.provider_class=org.hibernate.service.jdbc.connections.internal.C3P0ConnectionProvider
hibernate.dialect=org.hibernate.dialect.MySQL5Dialect
hibernate.show_sql=false
hibernate.hbm2ddl.auto=update
hibernate.format_sql=true
#开启二级缓存
hibernate.cache.use_second_level_cache=true
#设置二级缓存的提供者
hibernate.cache.region.factory_class=org.hibernate.cache.ehcache.EhCacheRegionFactory